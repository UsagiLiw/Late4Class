<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/classFrame"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.046"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.027"
        app:srcCompat="@drawable/dashboard_roadcornerbotton" />

    <TextView
        android:id="@+id/Profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CPE123"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="@color/textColor_White"
        android:textSize="25sp"
        android:visibility="visible"
        app:fontFamily="@font/calibri_bold"

        app:layout_constraintBottom_toBottomOf="@+id/classFrame"
        app:layout_constraintEnd_toEndOf="@+id/classFrame"
        app:layout_constraintStart_toStartOf="@+id/classFrame"
        app:layout_constraintTop_toTopOf="@+id/classFrame" />

    <ImageView
        android:id="@+id/classFrame2"
        android:layout_width="250dp"
        android:layout_height="70dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/classFrame"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.027"
        app:srcCompat="@drawable/rcgraybox" />

    <TextView
        android:id="@+id/textClassName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Quantum Computing"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/classFrame2"
        app:layout_constraintEnd_toEndOf="@+id/classFrame2"
        app:layout_constraintHorizontal_bias="0.262"
        app:layout_constraintStart_toStartOf="@+id/classFrame2"
        app:layout_constraintTop_toTopOf="@+id/classFrame2"
        app:layout_constraintVertical_bias="0.209" />

    <TextView
        android:id="@+id/textClassTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1000-1500"
        app:layout_constraintBottom_toBottomOf="@+id/classFrame2"
        app:layout_constraintEnd_toEndOf="@+id/classFrame2"
        app:layout_constraintHorizontal_bias="0.917"
        app:layout_constraintStart_toStartOf="@+id/classFrame2"
        app:layout_constraintTop_toTopOf="@+id/classFrame2"
        app:layout_constraintVertical_bias="0.862" />


</androidx.constraintlayout.widget.ConstraintLayout>