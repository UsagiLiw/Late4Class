<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.late4class">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".welcome"
            android:screenOrientation="portrait"
            android:theme="@style/LoginPage">
            <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Dashboard_pack.Dashboard"
            android:screenOrientation="portrait"
            android:theme="@style/DashBoard" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/LoginPage" />
        <activity
            android:name=".timeTable"
            android:screenOrientation="portrait"
            android:label="@string/timetable"
            android:theme="@style/TimeTableTheme"/>
        <activity
            android:name=".register"
            android:screenOrientation="portrait"
            android:label="@string/register"
            android:theme="@style/LoginPage"/>

    </application>

</manifest>